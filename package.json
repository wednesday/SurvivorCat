{
  "name": "neko",
  "version": "1.0.0",
  "description": "2D game project using Phaser framework",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build",
    "tauri:build:mac": "tauri build --target aarch64-apple-darwin",
    "tauri:build:mac-intel": "tauri build --target x86_64-apple-darwin",
    "tauri:build:win": "tauri build --target x86_64-pc-windows-msvc",
    "tauri:build:win32": "tauri build --target i686-pc-windows-msvc",
    "tauri:build:linux": "tauri build --target x86_64-unknown-linux-gnu",
    "tauri:build:all-win": "npm run tauri:build:win && npm run tauri:build:win32"
  },
  "dependencies": {
    "@tauri-apps/plugin-fs": "^2.4.4",
    "phaser": "^3.80.0"
  },
  "devDependencies": {
    "@tauri-apps/api": "^2.9.1",
    "@tauri-apps/cli": "^2.9.5",
    "typescript": "^5.0.0",
    "vite": "^5.0.0"
  }
}
